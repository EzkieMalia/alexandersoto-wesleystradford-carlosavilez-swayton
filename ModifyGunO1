local Secure = setmetatable({}, {
    __index = function(Idx, Val)
        return game:GetService(Val)
    end
})

local fireratespeed = 2000

local CoreGui = Secure.CoreGui

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = CoreGui

local Frame = Instance.new("Frame")
Frame.Parent = ScreenGui
Frame.Size = UDim2.new(.18, 0, .175, 0)
Frame.Position = UDim2.new(.325, 0, .425, 0)
Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

local FrameStroke = Instance.new("UIStroke")
FrameStroke.Parent = Frame
FrameStroke.Thickness = 3

local FrameCorner = Instance.new("UICorner")
FrameCorner.Parent = Frame
FrameCorner.CornerRadius = UDim.new(.05, 0)

local Drag = Instance.new("UIDragDetector")
Drag.Parent = Frame

local TextLabel = Instance.new("TextLabel")
TextLabel.Parent = Frame
TextLabel.Size = UDim2.new(.9, 0, .4, 0)
TextLabel.Position = UDim2.new(.05, 0, .1, 0)
TextLabel.Text = "Firerate speed: " .. fireratespeed
TextLabel.TextScaled = true
TextLabel.BackgroundTransparency = 1
TextLabel.TextColor3 = Color3.fromRGB(255,255,255)
TextLabel.Font = Enum.Font.FredokaOne

local AddSpeed = Instance.new('TextButton')
AddSpeed.Parent = Frame
AddSpeed.Text = "Add"
AddSpeed.Size = UDim2.new(.4, 0, .3, 0)
AddSpeed.Position = UDim2.new(.525, 0, .55, 0)
AddSpeed.BackgroundColor3 = Color3.fromRGB(0,255,0)
AddSpeed.TextScaled = true
AddSpeed.Font = Enum.Font.FredokaOne
AddSpeed.TextColor3 = Color3.fromRGB(255,255,255)

local AddSpeedCorner = Instance.new("UICorner")
AddSpeedCorner.Parent = AddSpeed
AddSpeedCorner.CornerRadius = UDim.new(.1, 0)

local RemoveSpeed = Instance.new('TextButton')
RemoveSpeed.Parent = Frame
RemoveSpeed.Text = "Minus"
RemoveSpeed.Size = UDim2.new(.4, 0, .3, 0)
RemoveSpeed.Position = UDim2.new(.075, 0, .55, 0)
RemoveSpeed.BackgroundColor3 = Color3.fromRGB(255,0,0)
RemoveSpeed.TextScaled = true
RemoveSpeed.Font = Enum.Font.FredokaOne
RemoveSpeed.TextColor3 = Color3.fromRGB(255, 255, 255)

local RemoveSpeedCorner = Instance.new("UICorner")
RemoveSpeedCorner.Parent = RemoveSpeed
RemoveSpeedCorner.CornerRadius = UDim.new(.1, 0)

local DestroyGui = Instance.new('TextButton')
DestroyGui.Parent = Frame
DestroyGui.Text = "Destroy Gui"
DestroyGui.Size = UDim2.new(.9, 0, .15, 0)
DestroyGui.Position = UDim2.new(.05, 0, .05, 0)
DestroyGui.BackgroundColor3 = Color3.fromRGB(255,0,0)
DestroyGui.TextScaled = true
DestroyGui.Font = Enum.Font.FredokaOne
DestroyGui.TextColor3 = Color3.fromRGB(255, 255, 255)

local DestroyGuiCorner = Instance.new("UICorner")
DestroyGuiCorner.Parent = DestroyGui
DestroyGuiCorner.CornerRadius = UDim.new(.1, 0)

DestroyGui.MouseButton1Click:Connect(function()
	ScreenGui:Destroy()
	fireratespeed = 750
end)

AddSpeed.MouseButton1Click:Connect(function()
if fireratespeed ~= 5000 then
	fireratespeed = fireratespeed + 100
	TextLabel.Text = "Firerate speed: " .. fireratespeed
end
end)

RemoveSpeed.MouseButton1Click:Connect(function()
if fireratespeed ~= 0 then
	fireratespeed = fireratespeed - 100
	TextLabel.Text = "Firerate speed: " .. fireratespeed
end
end)

while true do
local Actors = getactors()[1];
run_on_actor(Actors, [[
for _,v in next, getgc(true) do
    if (typeof(v) ~= "table") then
        continue;
    end;

    local Key = rawget(v, "flash");
    if (Key) then
        local oldfnc; oldfnc = hookfunction(v.recoil_function, function(gun_table, gun_instance)
            local weapon_states = gun_table["object"]["states"];
            weapon_states.recoil_up:set(0);
            weapon_states.recoil_side:set(0);
            weapon_states.firerate:set(fireratespeed);
            return oldfnc(gun_table, gun_instance);
        end);
    end;
end;
]])
task.wait(1.5)
end
